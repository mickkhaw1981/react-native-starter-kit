# Database URL
DATABASE_URL=postgresql://postgres:[YOUR_PASSWORD]@[YOUR_HOST]:[PORT]/[DB_NAME]
DIRECT_URL=postgresql://postgres:[YOUR_PASSWORD]@[YOUR_HOST]:[PORT]/[DB_NAME]

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# JWT Configuration
SUPABASE_JWT_SECRET=your_jwt_secret_at_least_32_chars
# Supabase Service Role (Keep this secret, backend-only)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# S3 Storage Configuration
S3_STORAGE_URL=your_s3_storage_url
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key
S3_REGION=your_s3_region

# Other
OPENAI_API_KEY=your-openai-api-key
GROQ_API_KEY=your-groq-api-key
EXPO_PUBLIC_APPWRITE_PROJECT_ID=your-appwrite-project-id
EXPO_PUBLIC_APPWRITE_REVIEWS_COLLECTION_ID=your_reviews_collection_id

